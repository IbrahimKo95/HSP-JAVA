      <Button fx:id="showButton" disable="true" layoutX="425.0" layoutY="217.0" mnemonicParsing="false" onAction="#showProducts" prefHeight="26.0" prefWidth="106.0" text="Voir les produits" />
   </children>
</AnchorPane>
