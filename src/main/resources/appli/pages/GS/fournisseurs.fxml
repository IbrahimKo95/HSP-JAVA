<AnchorPane fx:id="FournisseursPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="526.0" prefWidth="588.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.GS.FournisseurController">
   <children>
      <Label layoutX="241.0" layoutY="35.0" text="Fournisseurs">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ListView fx:id="list" layoutX="56.0" layoutY="257.0" onMouseClicked="#selectItems" prefHeight="270.0" prefWidth="476.0" />
